import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        int number;

        do {
            number = scanner.nextInt();
            if (number != -1) {
                sum += number;
            }
        } while (number != -1);

        System.out.println(sum);
        scanner.close();
    }
}
